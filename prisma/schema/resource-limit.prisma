model ResourceLimitModel {
  id       String       @id @default(uuid()) @db.Uuid
  type     ResourceType
  limit    Int
  used     Int
  domainId String       @db.Uuid
  domain   DomainModel  @relation(fields: [domainId], references: [id])

  @@unique([domainId, type])
}

enum ResourceType {
  INSTANCES
  CPU
  MEMORY
  VOLUMES
  PUBLICIP
  NETWORK
}
