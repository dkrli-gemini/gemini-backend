model AclListModel {
    id String @id() @default(uuid()) @db.Uuid
    name String
    description String? 

    vpc VPCModel? @relation(references: [id], fields: [vpcId])
    vpcId String? @db.Uuid
    
    AclRule AclRuleModel[]
}

model AclRuleModel {
    id String @id @default(uuid()) @db.Uuid
    acl AclListModel? @relation(fields: [aclId], references: [id])
    aclId String @db.Uuid
    cidrList String[]
    action AclActionModel
    startPort String
    endPort String
    trafficType AclTrafficType
    protocol NetworkProtocolModel
    description String? 
}

enum NetworkProtocolModel {
    TCP
    UDP
}

enum AclTrafficType {
    ingress
    egress
}

enum AclActionModel {
    allow
    deny
}