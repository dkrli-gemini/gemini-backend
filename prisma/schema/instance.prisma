model InstanceModel {
  id                  String                @id @default(uuid()) @db.Uuid
  name                String
  memory              String
  disk                String
  cpu                 String
  sku                 String?
  family              String?
  profile             String?
  diskTier            String?
  cpuNumber           Int
  cpuSpeedMhz         Int
  memoryInMb          Int
  rootDiskSizeInGb    Int
  VirtualMachineModel VirtualMachineModel[]
}
