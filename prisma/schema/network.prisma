model NetworkModel {
  id                String        @id @default(uuid()) @db.Uuid
  name              String
  cloudstackOfferId String
  cloudstackAclId   String
  aclName           String?
  gateway           String
  netmask           String
  projectId         String?       @db.Uuid
  project           ProjectModel? @relation(fields: [projectId], references: [id])
}
