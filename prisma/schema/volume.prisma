model VolumeModel {
    id String @id() @default(uuid()) @db.Uuid

    name String

    offer DiskOfferModel? @relation(fields: [offerId], references: [id])
    offerId String? @db.Uuid

    machine VirtualMachineModel? @relation(fields: [machineId], references: [id])
    machineId String? @db.Uuid
}