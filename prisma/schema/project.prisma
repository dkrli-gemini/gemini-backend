model ProjectModel {
  id                  String                @id @default(uuid()) @db.Uuid
  name                String
  type                ProjectTypeModel?
  domainId            String                @db.Uuid
  DomainMemberModel   DomainMemberModel[]
  NetworkModel        NetworkModel[]
  domain              DomainModel           @relation(fields: [domainId], references: [id])
  VirtualMachineModel VirtualMachineModel[]
  BillingEntryModel   BillingEntryModel[]
  ResourceUsageEventModel ResourceUsageEventModel[]

  PortForwardRuleModel PortForwardRuleModel[]
  PublicIPModel       PublicIPModel[]
}

enum ProjectTypeModel {
  ROOT
  MEMBER
}
